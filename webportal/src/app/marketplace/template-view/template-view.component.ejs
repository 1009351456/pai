<section class='content-header'>
  <%= breadcrumb({ breadcrumbTitle: "Marketplace" }) %>
</section>

<section class="content">
<div class="box">
  <div class="box-header">
    <button type="button" class="btn btn-success" id="btn-share">Share My Template!</button>
  </div>
  <div class="col-md-offset-2 col-md-8 search-container">
    <input type="text" placeholder="Search.." name="search">
    <button type="button"><i class="fa fa-search"></i></button>
  </div>
  <div id="table-view" class="clearfix">
    <%= template() %>
  </div>
</div>
<div id="modalPlaceHolder"></div>
<%= loading() %>
</section>
